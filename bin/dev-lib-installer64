#!/usr/bin/env bash
#
# Update dev-time external tool: Installer64
#
# * Template: 1.3.0
#

# shellcheck source=SCRIPTDIR/dev-set
source ./bin/dev-set || exit 1

[[ -z "$DEV_CICD_INSTALLER64_URL" ]] &&
  echo 'error: required variable not defined (DEV_CICD_INSTALLER64_URL)' &&
  exit 1

echo 'Deploy installer: Installer64'
cd "$DEV_LIB" &&
  curl -LO "$DEV_CICD_INSTALLER64_URL" &&
  chmod 755 "$DEV_CICD_INSTALLER64_BIN" &&
  "$DEV_CICD_INSTALLER64_BIN"
